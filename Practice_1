% 1
x =4; y = 16;
z = x + i*y;

% 2
z = 2+3j; % j 와 i 는 같은 의미
u = 2-5i;
v= 3 + 1j*5; % j 앞에 1을 써줘도 되고 안써도 된다.
real(z)
imag(u)
imag(v)

% 3
x = 4; y = 16;
z = x + i*y;
real(z) % 실수부
imag(z) % 허수부
conj(z) % 허수 부분의 부호가 바뀜

% 4
x = 4; y =16;
z = x + i*y;
angle(z)  % 각도
abs(z)    % 절댓값

% 5
z = 3 + i*2;
compass(z) % 좌표평면에 표현

% 6
z = 3 + i*2;
log(z)
sqrt(-1)
log(-1) % -1 의 자연 로그가 iπ임을 보여줍니다.
